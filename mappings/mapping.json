{
  "index_patterns": ["players_fifa_v1"],
  "template": {
    "settings": {
      "number_of_shards": 1,
      "number_of_replicas": 0
    },
    "mappings": {
      "_source": {
        "enabled": true
      },
      "properties": {
        "long_name": {
          "type": "text"
        },
        "overall": {
          "type": "integer"
        },
        "dob": {
          "type": "date",
          "format": "yyyy-MM-dd"
        }
      }
    }
  }
}
